{
  "name": "desktopCapture Example",
  "description": "Demonstrates how to use the chrome.desktopCapture API",
  "version": "1",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "sw/sw.js"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "desktopCapture",
    "offscreen"
  ]
}